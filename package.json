{
  "name": "start-next-app",
  "version": "0.1.0",
  "keywords": [
    "react",
    "next",
    "next.js"
  ],
  "description": "Create Next.js-powered React apps with one command",
  "author": "Raul Semicek Coelho",
  "license": "MIT",
  "bin": {
    "start-next-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "lint-fix": "prettier -w --plugin prettier-plugin-tailwindcss 'templates/*-tw/{ts,js}/{app,pages}/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@types/cross-spawn": "6.0.0",
    "@types/node": "^20.2.5",
    "@types/prompts": "2.0.1",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.34.0",
    "commander": "2.20.0",
    "cross-spawn": "7.0.3",
    "fast-glob": "3.3.1",
    "picocolors": "1.0.0",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "prompts": "2.1.0",
    "typescript": "^5.1.6",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
